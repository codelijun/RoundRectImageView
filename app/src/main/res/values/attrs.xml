<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--自定义MLImageView的属性 加上了自己的前缀，防止和其他自定义控件冲突-->
    <declare-styleable name="RoundRectImageView">
        <attr name="rr_radius" format="integer" />
        <attr name="rr_shape_type" format="enum">
            <enum name="none" value="0" />
            <enum name="round" value="1" />
            <enum name="rectangle" value="2" />
        </attr>
    </declare-styleable>
</resources>